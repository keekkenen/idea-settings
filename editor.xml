<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="15" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="JAVA" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="ARE_LINE_NUMBERS_SHOWN" value="false" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
    <option name="SHOW_BREADCRUMBS" value="false" />
    <option name="IS_CAMEL_WORDS" value="true" />
    <option name="IS_MOUSE_CLICK_SELECTION_HONORS_CAMEL_WORDS" value="false" />
    <option name="SHOW_NOTIFICATION_AFTER_REFORMAT_CODE_ACTION" value="false" />
    <option name="SHOW_NOTIFICATION_AFTER_OPTIMIZE_IMPORTS_ACTION" value="false" />
    <option name="SHOW_PARAMETER_NAME_HINTS" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bFIXME\b.*" />
    <pattern icon="question" useCustomColors="true" pattern="\bTODO\b.*\bkee\b.*">
      <option name="FOREGROUND" value="d2d2d2" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_COLOR" value="a94e7" />
      <option name="EFFECT_TYPE" value="5" />
    </pattern>
    <pattern icon="important" useCustomColors="true" pattern="\bFIXME\b.*\bkee\b.*">
      <option name="FOREGROUND" value="d2d2d2" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_COLOR" value="a94e7" />
      <option name="EFFECT_TYPE" value="5" />
    </pattern>
    <pattern icon="question" pattern="\bDEV\b.*" />
    <pattern useCustomColors="true" pattern="\bDEV\b.*\bkee\b.*">
      <option name="FOREGROUND" value="d2d2d2" />
      <option name="FONT_TYPE" value="2" />
      <option name="EFFECT_COLOR" value="a94e7" />
      <option name="EFFECT_TYPE" value="5" />
    </pattern>
    <filter name="my">
      <pattern index="2" />
      <pattern index="3" />
      <pattern index="5" />
    </filter>
  </component>
</application>